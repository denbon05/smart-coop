{
  "name": "smart-coop",
  "version": "1.0.0",
  "description": "Housing cooperatives manager project addresses transparency and decision-making issues within cooperative communities",
  "keywords": [
    "blockchain",
    "smart-contract",
    "house-cooperatives",
    "cooperative",
    "house"
  ],
  "author": "denbon05",
  "license": "MIT",
  "scripts": {
    "dev": "npx vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "npx vite preview",
    "test:contracts": "npx hardhat test",
    "test:unit": "npx vitest",
    "test": "npm run test:unit && npm run test:contracts",
    "build-only": "npx vite build",
    "type-check": "npx vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint-fix": "npx eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "npx prettier --write src/"
  },
  "dependencies": {
    "@mdi/font": "^7.2.96",
    "@openzeppelin/contracts": "^5.0.0",
    "alchemy-sdk": "^2.10.1",
    "ethers": "^6.7.1",
    "vee-validate": "^4.11.8",
    "vue": "^3.3.4",
    "vue-router": "^4.2.4",
    "vuetify": "^3.3.19"
  },
  "devDependencies": {
    "@metamask/providers": "^13.0.0",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.2",
    "@types/chai": "^4.3.7",
    "@types/jsdom": "^21.1.3",
    "@types/mocha": "^10.0.2",
    "@types/node": "^18.18.4",
    "@vitejs/plugin-vue": "^4.3.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "chai": "^4.3.10",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "hardhat": "^2.17.4",
    "jsdom": "^22.1.0",
    "npm-run-all2": "^6.0.6",
    "prettier": "^3.0.3",
    "sass": "^1.69.1",
    "ts-node": "^10.9.1",
    "typescript": "~5.2.0",
    "vite": "^4.4.9",
    "vitest": "^0.34.4",
    "vue-tsc": "^1.8.11"
  }
}
